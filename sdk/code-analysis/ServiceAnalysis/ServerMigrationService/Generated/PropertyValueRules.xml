    <property>Amazon.ServerMigrationService.Model.SSMValidationParameters.Command</property>
    <min>1</min>
    <max>64000</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.SSMValidationParameters.ExecutionTimeoutSeconds</property>
    <min>60</min>
    <max>28800</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.SSMValidationParameters.InstanceId</property>
    <pattern>(^i-(\w{8}|\w{17})$)|(^mi-\w{17}$)</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.ValidationOutput.Name</property>
    <min>1</min>
    <max>255</max>
    <pattern>^[\S]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.ValidationOutput.StatusMessage</property>
    <max>2500</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.ValidationOutput.ValidationId</property>
    <pattern>^val-[0-9a-f]{17}$</pattern>
  </property-value-rule>
</property-value-rules>