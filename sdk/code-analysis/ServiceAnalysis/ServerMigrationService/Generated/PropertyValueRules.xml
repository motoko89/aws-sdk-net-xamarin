<?xml version="1.0" encoding="utf-16"?>
<property-value-rules>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.DeleteAppValidationConfigurationRequest.AppId</property>
    <pattern>^app-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.GetAppValidationConfigurationRequest.AppId</property>
    <pattern>^app-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
