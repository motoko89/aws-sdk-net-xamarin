    <pattern>^app-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.NotifyAppValidationOutputRequest.AppId</property>
    <pattern>^app-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.PutAppValidationConfigurationRequest.AppId</property>
    <pattern>^app-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.AppValidationConfiguration.Name</property>
    <min>1</min>
    <max>255</max>
    <pattern>^[\S]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.AppValidationConfiguration.ValidationId</property>
    <pattern>^val-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.NotificationContext.StatusMessage</property>
    <max>2500</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.NotificationContext.ValidationId</property>
    <pattern>^val-[0-9a-f]{17}$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.S3Location.Bucket</property>
    <min>3</min>
    <max>63</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.S3Location.Key</property>
    <max>1024</max>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.ServerValidationConfiguration.Name</property>
    <min>1</min>
    <max>255</max>
    <pattern>^[\S]+$</pattern>
  </property-value-rule>
  <property-value-rule>
    <property>Amazon.ServerMigrationService.Model.ServerValidationConfiguration.ValidationId</property>
    <pattern>^val-[0-9a-f]{17}$</pattern>
  </property-value-rule>
